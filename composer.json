{
    "name": "amgportalapp/activity-log",
    "description": "User activity log plugin for AMGPortal.",
    "keywords": [
        "laravel",
        "amgportal",
        "activity log",
        "user activity"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2|^8.1",
        "illuminate/http": "^9.0",
        "amgportalapp/plugins": "dev-master",
        "spatie/laravel-query-builder": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "AMGPortal\\UserActivity\\": "src/",
            "AMGPortal\\UserActivity\\Database\\Factories\\": "database/factories/",
            "AMGPortal\\UserActivity\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AMGPortal\\UserActivity\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
